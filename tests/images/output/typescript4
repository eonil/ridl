/// Here be dragons.
/// Features below this line are currently supported.
type Tuna = string

type Salmon = string

/// Ingredients of magic.
/// 
enum Mineral {
    Iron = "Iron",
    Alumina = "Alumina",
    Arcana = "Arcana"  
}

/// Edible objects.
/// 
type Fish = { Tuna: Tuna } | { Salmon: Salmon }

/// 
type Pet = {
    name: string[]
    /// Did they take a walk today?
    walk: boolean
    living_address?: Address
}

/// 
type Address = {
    city: string
}

/// 
type APIError = {
    message: string
}
