/// Here be dragons.
/// Features below this line are currently supported.
type Tuna = string

type Salmon = string

/// Ingredients of magic.
enum Mineral {
    IronMetal = "IronMetal",
    Alumina = "Alumina",
    Arcana = "Arcana"  
}

/// Edibles.
type Dish = { Sushi: Tuna } | { PanFriedSteak: Salmon }

type Pet = {
    name: string[]
    /// Did they take a walk today?
    walk: boolean
    living_address?: Address
}

type Address = {
    city: string
}

type APIError = {
    message: string
}
